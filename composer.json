{
    "name": "somnambulist/read-models-bundle",
    "type": "library",
    "description": "Symfony integration for somnambulist/read-models",
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "info@somnambulist.tech"
        }
    ],
    "require": {
        "php": ">=7.4",
        "somnambulist/read-models": "^2.0",
        "symfony/framework-bundle": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "doctrine/doctrine-bundle": "~2.1",
        "fzaninotto/faker": "~1.8",
        "somnambulist/domain": "~3.0",
        "symfony/dotenv": "^5.1",
        "symfony/messenger": "^5.1",
        "symfony/stopwatch": "^5.1",
        "symfony/var-dumper": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\Bundles\\ReadModelsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Bundles\\ReadModelsBundle\\Tests\\": "tests/"
        }
    }
}
