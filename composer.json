{
    "name": "somnambulist/read-models-bundle",
    "type": "symfony-bundle",
    "description": "Symfony integration for somnambulist/read-models",
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "info@somnambulist.tech"
        }
    ],
    "require": {
        "php": ">=8.1",
        "somnambulist/read-models": "^4.1",
        "symfony/framework-bundle": "^6.4|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "doctrine/doctrine-bundle": "^2.11",
        "somnambulist/domain": "^6.0",
        "symfony/dotenv": "^6.4",
        "symfony/messenger": "^6.4",
        "symfony/stopwatch": "^6.4",
        "symfony/var-dumper": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\Bundles\\ReadModelsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Bundles\\ReadModelsBundle\\Tests\\": "tests/"
        }
    }
}
